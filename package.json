{
  "name": "@greenkeeper/jobs",
  "version": "0.0.0-development",
  "dependencies": {
    "@octokit/rest": "15.1.7",
    "amqplib": "^0.5.0",
    "bluebird": "^3.4.6",
    "catbox": "^7.1.3",
    "catbox-memory": "^2.0.4",
    "couchdb-bootstrap": "^14.2.0",
    "envalid": "^4.0.1",
    "escape-string-regexp": "^1.0.5",
    "github-url-from-git": "^1.4.0",
    "gk-log": "1.2.0",
    "hot-shots": "^5.0.0",
    "js-yaml": "^3.7.0",
    "json-in-place": "^1.0.1",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.16.1",
    "nodemailer": "^4.0.1",
    "npm-registry-client": "^8.3.0",
    "pouchdb-http": "^6.0.2",
    "pouchdb-mapreduce": "^6.0.5",
    "pouchdb-upsert": "^2.2.0",
    "promise-queue": "^2.2.3",
    "promise-retry": "^1.1.1",
    "readme-badger": "^0.2.0",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "retry-promise": "^1.0.0",
    "rollbar": "^2.0.3",
    "semver": "^5.3.0",
    "stripe": "^5.3.0",
    "yml-in-place": "^1.0.2"
  },
  "devDependencies": {
    "jest": "^22.4.2",
    "nock": "^9.0.0",
    "prettier-standard-formatter": "^0.222222222222222.333333333333333",
    "simple-mock": "^0.8.0",
    "standard": "^10.0.2"
  },
  "engines": {
    "node": ">=7.6"
  },
  "license": "Apache-2.0",
  "jest": {
    "testRegex": "/test/.*\\.js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/helpers/.*\\.js$"
    ],
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/nock"
    ],
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenkeeperio/greenkeeper.git"
  },
  "scripts": {
    "format": "prettier-standard-formatter jobs lib test",
    "db:start": "./start-couchdb",
    "deploy": "./deploy",
    "pretest": "standard && npm run db:start",
    "start": "node index.js",
    "test": "NODE_ENV=testing jest -i"
  },
  "standard": {
    "env": {
      "jest": true
    },
    "globals": [ "jest", "expect", "describe", "test", "beforeAll", "beforeEach", "afterAll", "afterEach"],
    "ignore": [
      "couchdb"
    ]
  }
}
